{
  "name": "bclibraries/libkey-client",
  "description": "PHP client for LibKey API from Third Iron",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Ben Florin",
      "email": "benjamin.florin@bc.edu"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "symfony/http-client": "^5.1",
    "nyholm/psr7": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "codedungeon/phpunit-result-printer": "^0.32.0",
    "php-http/mock-client": "^1.3",
    "mockery/mockery": "^1.3"
  },
  "config": {
  },
  "autoload": {
    "psr-4": {
      "BCLib\\LibKeyClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BCLib\\LibKeyClient\\Tests\\": "tests/"
    }
  }
}
